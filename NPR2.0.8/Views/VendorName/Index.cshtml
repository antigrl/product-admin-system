@model IEnumerable<NPR2._0._8.Models.VendorName>

@{
    ViewBag.Title = "Vendor Name Index";
}

<h2>Vendor Name Index</h2>

<div class="left-half-content">
    <p>
        @Html.ActionLink("Create New Vendor Name", "Create")
    </p>
    <table class="index-table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VendorNameName)
            </th>
            <th></th>
        </tr>

        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.VendorNameName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.VendorNameID }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.VendorNameID })
                </td>
            </tr>
        }

    </table>
</div>
