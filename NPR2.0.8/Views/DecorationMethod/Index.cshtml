@model IEnumerable<NPR2._0._8.Models.DecorationMethod>

@{
    ViewBag.Title = "Decoration Method Index";
}

<h2>Decoration Method Index</h2>

<div class="left-half-content">
    <p>
        @Html.ActionLink("Create New Decoration Method", "Create")
    </p>
    <table class="index-table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DecorationMethodName)
            </th>
            <th></th>
        </tr>

        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.DecorationMethodName)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.DecorationMethodID }) |
            @Html.ActionLink("Delete", "Delete", new { id = item.DecorationMethodID })
                </td>
            </tr>
        }

    </table>

</div>
