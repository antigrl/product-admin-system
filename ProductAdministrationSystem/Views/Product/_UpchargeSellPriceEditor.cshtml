@model PAS.Models.UpchargeSellPrice
@using PAS.Helpers;
@using PAS.Models;

<li class="upcharge-sell-price">
    @using (Html.BeginCollectionItem("UpchargeSellPrices"))
    {
        <span>
            @Html.DisplayFor(model => model.UpchargeSellPriceName):
            @Html.DisplayFor(model => model.UpchargeSellPriceFinalAmount)
            @Html.TextBoxFor(model => model.UpchargeSellPriceFinalRoundedAmount, new { @class = "dollar-value" })
        </span>
        <span class="hidden-inputs">
            @Html.HiddenFor(model => model.UpchargeSellPriceID)
            @Html.HiddenFor(model => model.UpchargeSellPriceName)
            @Html.HiddenFor(model => model.UpchargeSellPriceLevel)
            @Html.HiddenFor(model => model.UpchargeSellPriceFinalAmount)
            @Html.HiddenFor(model => model.UpchargeSellPriceLocked)
            @Html.HiddenFor(model => model.UpchargeID)
            @Html.HiddenFor(model => model.UpchargeSellPriceStatus)
        </span>
    }
</li>
